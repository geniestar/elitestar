<div class="search-result theme_{{class}}">
    <div class="bar">
       <div class="left">
            <div class="theme-sprite theme-icon-packer"></div>
            <div class="title">{{backpacker.name}}</div>
            <img src="/img/flags/{{countryFlag}}.jpg">
            <div class="updated-time">{{updatedText}}</div>
       </div> 
       <div class="right-item">
           <div class="theme-sprite theme-icon-contact"></div> 
           <div class="white-bar"></div>
       </div>
    </div>
    <div class="content">
        <div class="left-content"> 
            <img src="{{photoUrl}}">
        </div>
        <div class="right-content">
            <div class="info-set">
                <div class="title">{{i18n.SEARCH_RESULT_TITLE_WANTED_SPACE}}</div>
                <div class="desc one-line-desc">{{place}}</div>
            </div>
            <div class="clean"></div>
            <div class="info-set">
                <div class="title">{{i18n.SEARCH_RESULT_TITLE_ARRIVAL}}</div>
                <div class="desc">{{arrivalTime}}</div>
                <div class="title two-column">{{i18n.SEARCH_RESULT_TITLE_DURATION}}</div>
                <div class="desc two-column">{{durationText}}</div>
            </div>
            <div class="info-set">
                <div class="title">{{i18n.SEARCH_RESULT_TITLE_ACCEPTABLE_RENT}}</div>
                <div class="desc">{{backpacker.rent_low}} {{i18n.SEARCH_RESULT_AUD}}</div>
                <div class="title two-column">{{i18n.SEARCH_RESULT_TITLE_BED}}</div>
                <div class="desc two-column">{{bedText}}</div>
            </div>
            <div class="clean"></div>
            <div class="info-set">
                <div class="title">{{i18n.SEARCH_RESULT_TITLE_EXPECTABLE}}</div>
                <div class="desc one-line-desc">
                    {{#additionalHelpArray}} 
                        <div class="listing-sprite listing-icon listing-{{class}}" data-additional="{{additional}}"></div>
                    {{/additionalHelpArray}}
                    {{^additionalHelpArray}}
                        {{i18n.SEARCH_RESULT_NONE}}
                    {{/additionalHelpArray}}
                </div>
            </div>
            <div class="info-set">
                <div class="title"> </div>
                <div class="desc one-line-desc">
                    {{#facilitiesArray}} 
                        <div class="theme-sprite listing-icon theme-icon-{{class}}" data-additional="{{additional}}"></div>
                    {{/facilitiesArray}}
                    {{^facilitiesArray}}
                        {{i18n.SEARCH_RESULT_NONE}}
                    {{/facilitiesArray}}
                </div>
            </div>
        </div>
        <div class="clean"></div>
    </div>
</div>
